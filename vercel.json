{
  "version": 2,
  "git": {
    "deploymentEnabled": {
      "main": true,
      "staging": true,
      "*": false
    }
  },
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 60
      }
    },
    {
      "src": "api/cron/process-narrative-jobs.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 60
      }
    }
  ],
  "routes": [
    {
      "src": "/api/cron/process-narrative-jobs",
      "dest": "/api/cron/process-narrative-jobs.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/process-narrative-jobs",
      "schedule": "* * * * *"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "VERCEL": "1"
  }
}