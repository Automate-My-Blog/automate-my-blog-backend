{
  "version": 2,
  "git": {
    "deploymentEnabled": {
      "main": true,
      "*": false
    }
  },
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "functions": {
    "index.js": {
      "maxDuration": 60
    }
  },
  "crons": [
    {
      "path": "/api/cron/process-narrative-jobs",
      "schedule": "* * * * *"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "VERCEL": "1"
  }
}